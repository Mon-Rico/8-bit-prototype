function _draw()
	cls()
	
	if scene == "title" then
		draw_title()
		
	elseif scene == "select" then
		draw_charselect()
		
	elseif scene == "game" then
		//game pause section
	   
		 if pause then
			 print("game paused",2,cam_y+50,6)
		 else
		//a 3-2-1 warning before starting again needed
			 
			 draw_hud()
		 end
		 
		//game over section 
		
		 if gameover == true then
		 	air_time-=air_time
		  draw_gameover()			   
	  end
   
	end
end 

function draw_charselect()
 cls()
	rect(0,127,cam_y+127,0,5,0)
	//arrow right
	spr(132,player.x-40,cam_y+50,2,2)
	//arrow left
	spr(134,player.x+20,cam_y+50,2,2)
	

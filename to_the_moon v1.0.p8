function create_txt(txt,x,y,c)
	clip(2,cam_y,0,0)
	local w, h = print(txt,0,0)
	clip()
	local text= {
		txt = txt,
		x = x,
		y = y,
		w = w,
		h = h,
		c = c,
		curr_w = 0
	}
	add(texts,text)
end


function update_txt(text)
	for t in all(texts) do  
		if t.curr_w < t.w then  
			t.curr_w+=1  
		end
	end
end



function rock_sp(e)
 if not gameover then
	 if e.y<cam_y+128 then
	
	  e.y+=0.2
	 
	  if e.y+e.height<cam_y then
 	  spr(78,e.x,cam_y) //warning sprite
   else
   	spr(93,e.x,e.y)
  	
   	//boundary for collision
//	   rect(e.x+2,e.y+2,
//	        e.x+e.width,
//	        e.y+e.height,7)
   end
 
  else
   e.on=false 
  end 
 end
end

function bat_sp(e) 
 if not gameover then
  if e.x+e.width<128 then
	 
	  e.x+=0.2
	 
	  if e.y+e.height<cam_y then
 	  spr(78,e.x,cam_y) //warning
 
	  else if flr(air_time%2) > 0 then
